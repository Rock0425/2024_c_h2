/******************************************************************************

printf("_____=%d\n"_____);
的空格填公式

*******************************************************************************/
#include <iostream>

int main()
{
    int x,y;
    
    printf("please input int x=");
    scanf("%d",&x);
    printf("please input int y=");
    scanf("%d",&y);
    printf("   =%d\n"   );

    return 0;
}

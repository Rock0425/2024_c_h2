/******************************************************************************

變數

*******************************************************************************/
#include <stdio.h>
int main()
{
    int i;
    float f;
    char  c;
    
    i = 10;
    f = 0.123456;
    c = 'h';
    
    printf("i = %d\n",i);
    printf("f = %d\n",f);
    printf("c = %d\n",c);
    return 0;
} 

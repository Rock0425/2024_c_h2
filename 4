/******************************************************************************

\t:空格
\\:輸出\
\":輸出"
*******************************************************************************/
#include <stdio.h>

int main()
{
    printf("Hello World\n \t ");
    printf("goodbye World\n \\ ");
    printf("goodmorning World\n \" ");
    printf("goodAfternoon World\n");
    printf("goodNight World\n");

    return 0;
